{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"../utils/request\";\nimport { ElMessage } from \"element-plus\";\nimport ValidCode from \"../components/ValidCode\";\nexport default {\n  name: \"LoginPage\",\n  components: {\n    ValidCode\n  },\n  data() {\n    return {\n      validCode: '',\n      //通过valicode获取的验证码\n      form: {},\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    createValidCode(data) {\n      this.validCode = data;\n    },\n    login() {\n      this.$refs['form'].validate(valid => {\n        if (valid) {\n          if (!this.form.validCode) {\n            ElMessage.error(\"请填写验证码\");\n            return;\n          }\n          if (this.form.validCode.toLowerCase() !== this.validCode.toLowerCase()) {\n            ElMessage.error(\"验证码错误\");\n            return;\n          }\n          request.post(\"user/login\", this.form).then(res => {\n            if (res.code == 0) {\n              ElMessage.success(\"登录成功\");\n              sessionStorage.setItem(\"user\", JSON.stringify(res.data)); //缓存用户信息\n              this.$router.push(\"/dashboard\");\n            } else {\n              ElMessage.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","ElMessage","ValidCode","name","components","data","validCode","form","rules","username","required","message","trigger","password","methods","createValidCode","login","$refs","validate","valid","error","toLowerCase","post","then","res","code","success","sessionStorage","setItem","JSON","stringify","$router","push","msg"],"sources":["D:\\vscodeworkspace\\test_vue\\my-vue-project\\src\\views\\LoginPage.vue"],"sourcesContent":["<template>\r\n<div  class=\"login-container\"  >\r\n    <el-form ref=\"form\" :model=\"form\"   :rules=\"rules\" class=\"login-page\">\r\n      <h2 class=\"title\" style=\"margin-bottom: 20px\">系统登录</h2>\r\n      <el-form-item prop=\"username\" >\r\n        <el-input v-model=\"form.username\"  clearable>\r\n          <template #prefix>\r\n            <el-icon class=\"el-input__icon\"><User /></el-icon>\r\n          </template>\r\n        </el-input>\r\n      </el-form-item>\r\n      <el-form-item prop=\"password\" size=\"large\">\r\n        <el-input v-model=\"form.password\"  clearable show-password>\r\n          <template #prefix>\r\n            <el-icon class=\"el-input__icon\"><Lock /></el-icon>\r\n          </template>\r\n        </el-input>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <div style=\"display: flex\">\r\n          <el-input  v-model=\"form.validCode\" style=\"width: 45%;\" placeholder=\"请输入验证码\"></el-input>\r\n          <ValidCode @input=\"createValidCode\" style=\"width: 50%\"/>\r\n        </div>\r\n      </el-form-item>\r\n      <el-form-item >\r\n        <el-button type=\"primary\"  style=\" width: 100%\" @click=\"login\">登 录</el-button>\r\n      </el-form-item>\r\n      <el-form-item><el-button type=\"text\">前往注册 >> </el-button></el-form-item>\r\n    </el-form>\r\n</div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport request from \"../utils/request\";\r\nimport {ElMessage} from \"element-plus\";\r\nimport ValidCode from \"../components/ValidCode\";\r\n\r\nexport default {\r\n  name: \"LoginPage\",\r\n  components:{\r\n    ValidCode\r\n  },\r\n  data() {\r\n    return {\r\n      validCode: '',//通过valicode获取的验证码\r\n      form: {},\r\n      rules: {\r\n        username: [\r\n          {\r\n            required: true,\r\n            message: '请输入用户名',\r\n            trigger: 'blur',\r\n          }\r\n        ],\r\n        password: [\r\n          {\r\n            required: true,\r\n            message: '请输入密码',\r\n            trigger: 'blur',\r\n          }\r\n        ]\r\n\r\n      }\r\n\r\n    }\r\n  },\r\n  methods: {\r\n    createValidCode(data){\r\n      this.validCode = data\r\n    },\r\n    login(){\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          if (!this.form.validCode) {\r\n            ElMessage.error(\"请填写验证码\")\r\n            return\r\n          }\r\n          if(this.form.validCode.toLowerCase() !== this.validCode.toLowerCase()) {\r\n            ElMessage.error(\"验证码错误\")\r\n            return\r\n          }\r\n\r\n          request.post(\"user/login\", this.form).then(res => {\r\n            if (res.code == 0) {\r\n              ElMessage.success(\"登录成功\")\r\n              sessionStorage.setItem(\"user\",JSON.stringify(res.data))//缓存用户信息\r\n              this.$router.push(\"/dashboard\")\r\n            } else {\r\n              ElMessage.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      })\r\n\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  position: fixed;\r\n  width: 100%;\r\n  height: 100vh;\r\n  background: url('../img/bg2.svg');\r\n  background-size: contain;\r\n  overflow: hidden;\r\n}\r\n.login-page {\r\n  border-radius: 5px;\r\n  margin: 180px auto;\r\n  width: 350px;\r\n  padding: 35px 35px 15px;\r\n  background: #fff;\r\n  border: 1px solid #eaeaea;\r\n  box-shadow: 0 0 25px #cac6c6;\r\n}\r\n</style>"],"mappings":";AAkCA,OAAOA,OAAM,MAAO,kBAAkB;AACtC,SAAQC,SAAS,QAAO,cAAc;AACtC,OAAOC,SAAQ,MAAO,yBAAyB;AAE/C,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAC;IACTF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MAAC;MACdC,IAAI,EAAE,CAAC,CAAC;MACRC,KAAK,EAAE;QACLC,QAAQ,EAAE,CACR;UACEC,QAAQ,EAAE,IAAI;UACdC,OAAO,EAAE,QAAQ;UACjBC,OAAO,EAAE;QACX,EACD;QACDC,QAAQ,EAAE,CACR;UACEH,QAAQ,EAAE,IAAI;UACdC,OAAO,EAAE,OAAO;UAChBC,OAAO,EAAE;QACX;MAGJ;IAEF;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,eAAeA,CAACV,IAAI,EAAC;MACnB,IAAI,CAACC,SAAQ,GAAID,IAAG;IACtB,CAAC;IACDW,KAAKA,CAAA,EAAE;MACL,IAAI,CAACC,KAAK,CAAC,MAAM,CAAC,CAACC,QAAQ,CAA